{
  "name": "@opd/g2plot-vue",
  "version": "1.0.0",
  "description": "G2Plot for vue 3",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "typings",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "es",
    "typings",
    "*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vitepress --root ./docs",
    "lint": "eslint . --ext=.ts,.tsx",
    "lint:fix": "eslint . --ext=.ts,.tsx --fix",
    "test": "jest",
    "prebuild": "npm test",
    "build": "npm run build:es & npm run build:commonjs & npm run build:types",
    "build:es": "babel src -d es --extensions .ts,.tsx --delete-dir-on-start",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src -d lib --extensions .ts,.tsx --delete-dir-on-start",
    "build:types": "tsc",
    "build:docs": "vitepress build --root ./docs",
    "preversion": "npm run build",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-data-plan/g2plot-vue.git"
  },
  "keywords": [
    "g2plot",
    "vue3",
    "vue",
    "g2",
    "chart"
  ],
  "author": "kagawagao <kingsongao1221@gmail.com> (https://kingsongao.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/open-data-plan/g2plot-vue/issues"
  },
  "homepage": "https://github.com/open-data-plan/g2plot-vue#readme",
  "devDependencies": {
    "@antv/g2plot": "^1.1.28",
    "@babel/cli": "^7.11.6",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@opd/jest-preset-pangu": "^1.8.3",
    "@opd/prettier-config-pangu": "^1.8.3",
    "@types/lodash": "^4.14.161",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "@vue/babel-preset-app": "^4.5.6",
    "@vue/test-utils": "^2.0.0-beta.5",
    "babel-eslint": "^10.1.0",
    "canvas": "^2.6.1",
    "conventional-changelog-cli": "^2.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^7.0.0-beta.4",
    "husky": "^4.3.0",
    "jest-canvas-mock": "^2.2.0",
    "lint-staged": "^10.4.0",
    "lodash": "^4.17.20",
    "typescript": "^4.0.3",
    "vitepress": "^0.6.0",
    "vue": "^3.0.0"
  },
  "dependencies": {},
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "prettier": "@opd/prettier-config-pangu"
}
